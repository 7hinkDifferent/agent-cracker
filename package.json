{
  "name": "agent-cracker",
  "private": true,
  "description": "Systematically study coding agents' implementation and reproduce key mechanisms",
  "scripts": {
    "postinstall": "npm run setup",
    "setup": "cp scripts/githooks/* .git/hooks/ && chmod +x .git/hooks/*",
    "add": "bash scripts/manage-submodules.sh add",
    "update": "bash scripts/manage-submodules.sh update",
    "init": "bash scripts/manage-submodules.sh init",
    "status": "bash scripts/manage-submodules.sh status",
    "readme": "bash scripts/gen-readme.sh",
    "stars": "bash scripts/update-stars.sh",
    "new-doc": "bash scripts/new-analysis.sh",
    "lint": "bash scripts/lint.sh",
    "progress": "bash scripts/gen-progress.sh"
  }
}
